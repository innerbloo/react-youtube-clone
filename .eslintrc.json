{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "camelcase": [0, {"properties": "always"}],
        "new-cap": 0,
        "no-unused-vars": "off",
        "react/jsx-uses-react": "off",
        "react/react-in-jsx-scope": "off",
        "require-jsdoc" : 0,
        "valid-jsdoc": 0
    }
}
